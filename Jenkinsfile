pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh 'echo "Hello, This is Build stage"'
            }
        }
        stage('Test') {
             steps {
                sh 'echo "Hello, This is Test stage"'
            }            
        }
        stage('Deploy') {
             steps {
                sh 'echo "Hello, This is Deploy stage"'
            }            
        }

    }
}